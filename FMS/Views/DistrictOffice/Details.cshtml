@model FMS.Models.DistrictOffice
@{
    ViewBag.Title = "District Office (Sector) Details";
}

<div class="title">
    @ViewBag.Title
    <div class="float-right">
        @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-info" })
    </div>
</div>

<div>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.DivisionalOffice)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DivisionalOffice.Place.Name)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Population)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Population)
        </dd>

    </dl>

    @Html.DisplayFor(model => model.Place)
        
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.DistrictOfficeId }) |
    @Html.ActionLink("Back to List", "Index")
</p>
